coroutine.wrap(function()
	while task.wait(0.1) do
		if getgenv().Table.GuiStatus.SwitchScriptCooldown >= 0.1 then
			getgenv().Table.GuiStatus.SwitchScriptCooldown += 0.1
		elseif getgenv().Table.GuiStatus.SwitchScriptCooldown <= 0 then
			getgenv().Table.GuiStatus.SwitchScriptCooldown = 0
		end
	end
end)()
